		"@swc/core": "^1.2.218",
		"@swc/jest": "^0.2.22",
		"@types/jest": "^27.5.1",
		"@types/lodash": "^4.14.182",
		"@types/node": "^16.11.45",
		"@types/uuid": "^8.3.4",
		"create-ts-index": "^1.14.0",
		"jest": "^27.5.1",
		"lodash": "^4.17.21",
		"regenerator-runtime": "^0.13.9",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4",
		"uuid": "^8.3.2"
	},
	"dependencies": {
		"class-validator": "^0.13.2"
